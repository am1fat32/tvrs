{
  "name": "tvrs",
  "version": "3.1.2",
  "description": "CLI tool for TV dev",
  "type": "module",
  "bin": "bin/tvrs.js",
  "scripts": {
    "test": "jest",
    "build": "tsup src/index.ts --format esm --outDir lib",
    "prepublishOnly": "npm run build",
    "biome:check": "npx @biomejs/biome check .",
    "biome:write": "npm run biome:check -- --write ."
