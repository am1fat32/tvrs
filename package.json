  "files": ["lib", "bin"],
  "keywords": ["tvrs"],
  "author": "Igor Kucherenko",
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0"
  }
}
